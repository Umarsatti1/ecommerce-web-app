{
    "containerDefinitions": [
        {
            "name": "ecommerce-app-container",
            "image": "730335208305.dkr.ecr.us-east-1.amazonaws.com/ecommerce-app:latest",
            "cpu": 1024,
            "memory": 3072,
            "memoryReservation": 1024,
            "portMappings": [
                {
                    "name": "container-port-protocol",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "SecretKey",
                    "value": "${{ secrets.AWS_SECRET_KEY }}"
                },
                {
                    "name": "DefaultConnection",
                    "value": "${{ secrets.AWS_DB_CONNECTION }}"
                },
                {
                    "name": "BucketName",
                    "value": "${{ secrets.AWS_BUCKET_NAME }}"
                },
                {
                    "name": "Region",
                    "value": "${{ secrets.AWS_REGION }}"
                },
                {
                    "name": "PublishableKey",
                    "value": "${{ secrets.AWS_PUBLISHABLE_KEY }}"
                },
                {
                    "name": "AccessKey",
                    "value": "${{ secrets.AWS_ACCESS_KEY }}"
                },
                {
                    "name": "WhSecret",
                    "value": "${{ secrets.AWS_WH_SECRET }}"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/ecommerce-app-task",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "family": "ecommerce-app-task",
    "taskRoleArn": "arn:aws:iam::730335208305:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::730335208305:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "volumes": [],
    "ephemeralStorage": {
        "sizeInGiB": 21
    }
}
